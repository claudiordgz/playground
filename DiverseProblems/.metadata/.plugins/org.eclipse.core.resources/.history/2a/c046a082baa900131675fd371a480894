#include <iostream>
#include <unordered_map>
#include <cstdint>
using namespace std;

enum class Unflavored : int8_t { cereal, pasta, lasagna, tortillas, batter, pita, END_OF_LIST };
enum class Salty { crackers, corn_chips, poutines, tacos, french_friend, END_OF_LIST };
enum class Sugary { doughnuts, pastries, cake, cupcakes, cookies, tarts, pies, END_OF_LIST };
enum class Sweet { waffles, pancakes, pikelets, pudding, souffles, fritters, muffins, END_OF_LIST };


int main() {
	int i = 5;
	auto e = i;
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
