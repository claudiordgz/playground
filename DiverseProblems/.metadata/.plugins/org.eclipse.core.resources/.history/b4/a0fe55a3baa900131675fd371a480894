#include <iostream>
#include <unordered_map>
#include <stdint.h>
using namespace std;

enum class Unflavored : uint8_t { cereal, pasta, lasagna, tortillas, batter, pita, END_OF_LIST };
enum class Salty : uint8_t { crackers, corn_chips, poutines, tacos, french_friend, END_OF_LIST };
enum class Sugary : uint8_t { doughnuts, pastries, cake, cupcakes, cookies, tarts, pies, END_OF_LIST };
enum class Sweet : uint8_t { waffles, pancakes, pikelets, pudding, souffles, fritters, muffins, END_OF_LIST };


int main() {
	int i = 5;
	auto e = i;
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
